{
    "multipart": [
        {
            "when": {
                "age": 0,
                "half": "lower"
            },
            "apply": [{"model": "block/%name%_bottom_stage_0"}]
        },
        {
            "when": {
                "age": 1,
                "half": "lower"
            },
            "apply": [{"model": "block/%name%_bottom_stage_1"}]
        },
        {
            "when": {
                "age": 2,
                "half": "lower"
            },
            "apply": [{"model": "block/%name%_bottom_stage_2"}]
        },
        {
            "when": {
                "age": 3,
                "half": "lower"
            },
            "apply": [{"model": "block/%name%_bottom_stage_3"}]
        },
        {
            "when": {
                "age": 3,
                "half": "upper"
            },
            "apply": [{"model": "block/%name%_top_stage_3"}]
        },
        {
            "when": {
                "age": 4,
                "half": "lower"
            },
            "apply": [{"model": "block/%name%_bottom_stage_4"}]
        },
        {
            "when": {
                "age": 4,
                "half": "upper"
            },
            "apply": [{"model": "block/%name%_top_stage_4"}]
        },
        {
            "when": {
                "age": 4,
                "half": "lower"
            },
            "apply": [{"model": "block/h_%name%"}]
        },
        {
            "when": {
                "age": 4,
                "half": "upper"
            },
            "apply": [{"model": "block/bang"}]
        }
    ]
}